<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="561.0" prefWidth="776.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.ShowReviewController">
   <children>
      <TableView fx:id="bookstbl" layoutY="26.0" prefHeight="331.0" prefWidth="776.0">
         <columns>
            <TableColumn fx:id="bookidclm" prefWidth="88.0" text="Book_id" />
            <TableColumn fx:id="usernameclm" prefWidth="133.0" text="username" />
            <TableColumn fx:id="starclm" prefWidth="122.0" text="Star" />
            <TableColumn fx:id="commentclm" prefWidth="425.0" text="Comment" />
         </columns>
         <effect>
            <DropShadow />
         </effect>
      </TableView>
      <Button fx:id="backbtn" layoutX="58.0" layoutY="385.0" mnemonicParsing="false" onAction="#back" prefHeight="27.0" prefWidth="106.0" style="-fx-background-color: #000000; -fx-cursor: hand;" text="Back" textFill="#296dda" underline="true">
         <font>
            <Font name="Arial Rounded MT Bold" size="14.0" />
         </font>
      </Button>
      <Button fx:id="logoutbtn" layoutX="559.0" layoutY="385.0" mnemonicParsing="false" onAction="#logout" prefHeight="27.0" prefWidth="106.0" style="-fx-background-color: #000000; -fx-cursor: hand;" text="Logout" textFill="#296dda" underline="true">
         <font>
            <Font name="Arial Rounded MT Bold" size="14.0" />
         </font>
      </Button>
      <TextField fx:id="avgstar" editable="false" layoutX="331.0" layoutY="386.0" />
      <Label layoutX="233.0" layoutY="390.0" prefHeight="21.0" prefWidth="88.0" text="Avg. Star">
         <font>
            <Font name="Arial Rounded MT Bold" size="17.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
